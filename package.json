{
  "name": "graphbook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client": "webpack serve --devtool inline-source-map --hot --config webpack.client.conf.js",
    "client:build": "webpack --config webpack.client.build.config.js",
    "server": "nodemon --exec babel-node --plugins require-context-hook --watch src/server src/server/index.js",
    "sqlz:undo-all-seed": "npx sequelize db:seed:undo:all --seeders-path src/server/seeders --config src/server/config/index.js",
    "sqlz:seed-all": "npx sequelize db:seed:all --seeders-path src/server/seeders --config src/server/config/index.js",
    "sqlz:migrate": "npx sequelize db:migrate --migrations-path src/server/migrations --config src/server/config/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@graphql-tools/schema": "^9.0.14",
    "apollo-server-express": "^3.11.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "mysql2": "^3.1.2",
    "nodemon": "^2.0.20",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.8.2"
  }
}
